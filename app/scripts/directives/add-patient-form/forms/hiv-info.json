{
  "key": "hivInfo",
  "name": "HIV Info",
  "displayName": "HIV Information",
  "flex": "100",
  "rows": [{
    "name": "Row 1",
    "title": "Please supply additional information below",
    "layout": "row",
    "fields": [{
      "type": "select",
      "flex": "33",
      "name": "hivStatus",
      "title": "HIV Status",
      "settings": {
        "valueType": "valueString",
        "disabled": false,
        "required": true
      },
      "FHIRMappings": [{
        "path": "extension.valueString",
        "params": "extension.url=rcbs:hiv:status",
        "valueType": "formValue",
        "value": "hivStatus"
      }],
      "options": [
        {"key": "Negative", "value": "Negative"},
        {"key": "Positive", "value": "Positive"}
      ],
      "value": null
    }, {
      "type": "date",
      "flex": "33",
      "name": "firstPostitiveHivTestDate",
      "title": "First HIV Postive Test Date",
      "settings": {
        "valueType": "valueString",
        "disabled": false,
        "required": true
      },
      "skipLogic": {
      	"func": null,
      	"checks": [{
      		"variable": "form.hivStatus.$modelValue",
      		"operand": "=",
      		"value": "Positive"
      	}]
      },
      "FHIRMappings": [{
        "path": "extension.valueDate",
        "params": "extension.url=rcbs:firstposivehivtest:date",
        "valueType": "formValue",
        "value": "firstPostitiveHivTestDate"
