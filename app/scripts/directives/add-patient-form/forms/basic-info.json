{
	"key": "basicInfo",
	"name": "Basic Info",
	"displayName": "Basic Information",
	"flex": "100",
	"rows": [{
		"name": "Row 1",
		"title": "Please supply your basic information below.",
		"layout": "row",
		"fields": [{
  		"type": "input",
  		"flex": "20",
  		"name": "TracNetID",
      "title": "TracNet ID",
      "settings": {
      	"required": false
      },
      "FHIRMappings": [{
				"resourceKey": "main",
        "path": "identifier[0].system",
        "params": null,
        "valueType": "staticValue",
        "value": "ocrvs:tracnet:id"
      }, {
				"resourceKey": "main",
        "path": "identifier[0].assigner.display",
        "params": null,
        "valueType": "staticValue",
        "value": "TracNet ID"
      }, {
				"resourceKey": "main",
      	"path": "identifier[0].value",
        "params": null,
      	"valueType": "formValue",
      	"value": "TracNetID"
      }]
    }, {
      "type": "input",
  		"flex": "20",
  		"name": "firstname",
      "title": "Given Name",
      "settings": {
      	"required": true
      },
      "FHIRMappings": [{
				"resourceKey": "main",
        "path": "name[0].given[0]",
        "params": null,
        "valueType": "formValue",
        "value": "firstname"
      }]
    }, {
      "type": "input",
  		"flex": "30",
  		"name": "middleNames",
      "title": "Middles Names",
      "settings": {
      	"required": false
      },
      "FHIRMappings": [{
				"resourceKey": "main",
        "path": "name[0].given[1]",
        "params": null,
        "valueType": "formValue",
        "value": "middleNames"
      }]
    }, {
      "type": "input",
  		"flex": "30",
  		"name": "lastName",
      "title": "Family Name",
      "settings": {
      	"required": true
      },
      "FHIRMappings": [{
				"resourceKey": "main",
        "path": "name[0].family[0]",
        "params": "",
        "valueType": "formValue",
        "value": "lastName"
      }]
    }, {
      "type": "select",
  		"flex": "20",
  		"name": "contactNumberType",
      "title": "Contact Number Type",
      "settings": {
      	"required": true
      },
      "options": [{"key": "Mobile",  "value": "Mobile"}, {"key": "Home",  "value": "Home"}, {"key": "Other",  "value": "Other"}],
      "FHIRMappings": [{
				"resourceKey": "main",
        "path": "telecom.use",
        "params": "telecom.system=phone",
        "valueType": "formValue",
        "value": "contactNumberType"
      }]
    }, {
      "type": "select",
  		"flex": "20",
  		"name": "gender",
      "title": "Gender",
      "settings": {
      	"required": true
      },
			"options": [
				 {
						"key": "male",
						"value": "Male"
				 },
				 {
						"key": "female",
						"value": "Female"
				 },
				 {
						"key": "other",
						"value": "Other"
				 },
				 {
						"key": "unknown",
						"value": "Unknown"
				 }
			],
      "FHIRMappings": [{
				"resourceKey": "main",
        "path": "gender",
        "params": null,
      	"valueType": "formValue",
      	"value": "gender"
      }]
    }, {
      "type": "select",
  		"flex": "20",
  		"name": "preferredLanguage",
      "title": "Preferred Language",
      "settings": {
      	"required": true
      },
      "options": [
				{"key": "Kinyarwanda",  "value": "Kinyarwanda"},
				{"key": "English",  "value": "English"},
				{"key": "French",  "value": "French"},
				{"key": "Swahili",  "value": "Swahili"},
				{"key": "Other",  "value": "Other"}
			],
      "FHIRMappings": [{
				"resourceKey": "main",
        "path": "communication[0].language.text",
        "params": null,
        "valueType": "formValue",
        "value": "preferredLanguage"
      }]
    }, {
      "type": "input",
  		"flex": "20",
  		"name": "preferredLanguageOther",
      "title": "Preferred Language (other)",
      "settings": {
      	"required": true
      },
      "skipLogic": {
      	"func": null,
      	"checks": [{
      		"variable": "form.preferredLanguage.$modelValue",
      		"operand": "=",
      		"value": "Other"
      	}]
      },
      "FHIRMappings": [{
				"resourceKey": "main",
        "path": "communication[0].language.text",
        "params": null,
        "valueType": "formValue",
        "value": "preferredLanguageOther"
      }]
    }]
	}]
}