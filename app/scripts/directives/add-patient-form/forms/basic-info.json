{
	"key": "basicInfo",
	"name": "Basic Info",
	"displayName": "Basic Information",
	"flex": "100",
	"rows": [{
		"name": "Row 1",
		"title": "Please supply your basic information below.",
		"layout": "row",
		"fields": [{
  		"type": "input",
  		"flex": "20",
  		"name": "TracNetID",
      "title": "TracNet ID",
      "settings": {
      	"required": true
      },
      "FHIRMappings": [{
        "path": "identifier.system",
        "params": null,
        "valueType": "staticValue",
        "value": "pshr:tracnet:id"
      }, {
        "path": "identifier.assigner.display",
        "params": null,
        "valueType": "staticValue",
        "value": "TracNet ID"
      }, {
      	"path": "identifier.value",
        "params": null,
      	"valueType": "formValue",
      	"value": "TracNetID"
      }]
    }, { 
      "type": "select",
  		"flex": "10",
  		"name": "title",
      "title": "Title",
      "settings": {
      	"required": true
      },
      "options": [{"key": "Mr",  "value": "Mr"}, {"key": "Mrs",  "value": "Mrs"}, {"key": "Ms",  "value": "Ms"}, {"key": "Dr",  "value": "Dr"}],
      "FHIRMappings": [{
        "path": "name.prefix[0]",
        "params": null,
        "valueType": "formValue",
        "value": "title"
      }]
    }, { 
      "type": "input",
  		"flex": "20",
  		"name": "firstname",
      "title": "First Name",
      "settings": {
      	"required": true
      },
      "FHIRMappings": [{
        "path": "name.given[0]",
        "params": null,
        "valueType": "formValue",
        "value": "firstname"
      }]
    }, { 
      "type": "input",
  		"flex": "20",
  		"name": "middleNames",
      "title": "Middles Names",
      "settings": {
      	"required": false
      },
      "FHIRMappings": [{
        "path": "name.given[1]",
        "params": null,
        "valueType": "formValue",
        "value": "middleNames"
      }]
    }, { 
      "type": "input",
  		"flex": "30",
  		"name": "lastName",
      "title": "Last Name",
      "settings": {
      	"required": true
      },
      "FHIRMappings": [{
        "path": "name.family[0]",
        "params": "",
        "valueType": "formValue",
        "value": "lastName"
      }]
    }, { 
      "type": "select",
  		"flex": "20",
  		"name": "contactNumberType",
      "title": "Contact Number Type",
      "settings": {
      	"required": true
      },
      "options": [{"key": "Mobile",  "value": "Mobile"}, {"key": "Home",  "value": "Home"}, {"key": "Other",  "value": "Other"}],
      "FHIRMappings": [{
        "path": "telecom.system",
        "params": "telecom.system=Mobile|:|Home|:|Other",
        "valueType": "formValue",
        "value": "contactNumberType"
      }]
    }, { 
      "type": "inputNumber",
  		"flex": "30",
  		"name": "contactNumber",
      "title": "Contact Number",
      "settings": {
      	"required": true
      },
      "FHIRMappings": [{
        "path": "telecom.value",
        "params": "telecom.system=phone",
        "valueType": "formValue",
        "value": "contactNumber"
      }]
    }, { 
      "type": "radio",
  		"flex": "20",
  		"name": "gender",
      "title": "Gender",
      "settings": {
      	"required": true
      },
      "options": [{"key": "male",  "value": "Male"}, {"key": "female",  "value": "Female"}],
      "FHIRMappings": [{
        "path": "gender",
        "params": null,
      	"valueType": "formValue",
      	"value": "gender"
      }]
    }, { 
      "type": "date",
  		"flex": "30",
  		"name": "dob",
      "title": "Date of Birth",
      "settings": {
				"valueType": "valueString",
        "disabled": false,
      	"required": true
      },
      "FHIRMappings": [{
        "path": "birthDate",
        "params": null,
      	"valueType": "formValue",
      	"value": "dob"
      }],
			"value": null
    }, { 
      "type": "email",
  		"flex": "50",
  		"name": "emailAddress",
      "title": "Email Address",
      "settings": {
      	"required": true
      },
      "FHIRMappings": [{
        "path": "telecom.value",
        "params": "telecom.system=email",
        "valueType": "formValue",
        "value": "emailAddress"
      }]
    }, { 
      "type": "email",
  		"flex": "50",
  		"name": "emaildAddressConfirm",
      "title": "Email Address Confirm",
      "settings": {
      	"required": true
      }
    }, { 
      "type": "select",
  		"flex": "20",
  		"name": "preferredLanguage",
      "title": "Preferred Language",
      "settings": {
      	"required": true
      },
      "options": [{"key": "English",  "value": "English"}, {"key": "Afrikaans",  "value": "Afrikaans"}, {"key": "Zulu",  "value": "Zulu"}, {"key": "Xhosa",  "value": "Xhosa"}, {"key": "Other",  "value": "Other"}],
      "FHIRMappings": [{
        "path": "communication[0].language.text",
        "params": null,
        "valueType": "formValue",
        "value": "preferredLanguage"
      }]
    }, { 
      "type": "input",
  		"flex": "30",
  		"name": "preferredLanguageOther",
      "title": "Preferred Language (other)",
      "settings": {
      	"required": true
      },
      "skipLogic": {
      	"func": null,
      	"checks": [{
      		"variable": "form.preferredLanguage.$modelValue",
      		"operand": "=",
      		"value": "Other"
      	}]
      },
      "FHIRMappings": [{
        "path": "",
        "params": null,
        "valueType": "formValue",
        "value": "preferredLanguageOther"
      }]
    }]
	}]
}